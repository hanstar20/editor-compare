<script>
  <!-- Initialize the editor. -->
  var editor = new FroalaEditor('#froala', {
    height: 500,
    heightMax: 500
  }, function() {
  })

  var editor2 = new FroalaEditor('#froala2', {
    // bottom toolbar hide
    toolbarBottom: true,

    // Characters count hide
    charCounterCount: false,

    // height setting
    height: 500,
    heightMax: 500

  } ,function () {
    // read only mode
    editor2.edit.off();

    // toolbar hide
    editor2.toolbar.hide();
  })

  var editor3 = new FroalaEditor('#froala3', {
  }, function() {
  })

  var editor4 = new FroalaEditor('#froala4', {
    // bottom toolbar hide
    toolbarBottom: true,

    // Characters count hide
    charCounterCount: false
  } ,function () {
    // read only mode
    editor4.edit.off();

    // toolbar hide
    editor4.toolbar.hide();
  })

  document.getElementById('btn').addEventListener('click', function (){
    console.log(editor.html.get(true))
  })

  document.getElementById('froala').addEventListener('keydown', function (){
    console.log("키보드 입력중")
  })

