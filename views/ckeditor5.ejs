<div>
    <h2> return check btn</h2>
    <button id="btn">
        버튼
    </button>
</div>

<script>
    let editor;

    ClassicEditor
      .create( document.querySelector( '#editor1' ) )
      .then( newEditor => {
        editor = newEditor;
        // keydown event 입력 받기
        newEditor.editing.view.document.on( 'keydown', ( evt, data ) => {
          console.log(data.domEvent.key);
        })
      } )
      .catch( error => {
        console.error( error );
      } );

    // editor에 쓰여진 내용 받아오기
    document.getElementById('btn').addEventListener('click', function (){
      const editorData = editor.getData();
      console.log(editorData)
    })

    ClassicEditor
      .create( document.querySelector( '#editor2' ) )
      .then( editor => {

        editor.isReadOnly = true;
      } )
      .catch( error => {
        console.error( error );
      } );


</script>

